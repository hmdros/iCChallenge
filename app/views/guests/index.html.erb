<div class="container">
  <div class="row">
    <table class="table">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">UUID</th>
          <th scope="col">URL</th>
          <th scope="col">Data | Hora</th>
        </tr>
      </thead>
      <tbody>
        <% @informations.each do |information| %>
        <tr class="information">
          <th scope="row"><%= information.id %></th>
          <td><%= information.uuid %></td>
          <td>@<%= information.url %></td>
          <td><%= information.timestamp %></td>
          <td><%= link_to 'Show', information %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>

